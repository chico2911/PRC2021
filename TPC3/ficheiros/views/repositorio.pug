extends layout

block content
  .w3-card-4
        header.w3-container.w3-teal
            h3.w3-center #{rep.id} 
  .w3-container
    .w3-card-4
      header.w3-container.w3-teal.w3-margin-top
        h4 Nome de Classes
      table.w3-table-all
